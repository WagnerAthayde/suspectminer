<h1>Log page for suspect <%= @email %></h1>

<table>
	<thead>
		<tr>
			<td>URL:</td>
			<td>At:</td>
		</tr>
	</thead>
	<tbody>
		<% @accesses.each do |access| %>
		<tr>
			<td><%= access.url %></td>
			<td><%= access.created_at%></td>
		</tr>
		<% end %>
	</tbody>
</table>

<a href="/">Suspects List</a>